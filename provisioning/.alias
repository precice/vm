# preCICE config visualizer
function preciceToPNG(){
  precice-config-visualizer "${1:-precice-config.xml}" | dot -Tpng > precice-config.png
}

function preciceToPDF(){
  precice-config-visualizer "${1:-precice-config.xml}" | dot -Tpdf > precice-config.pdf
}

function preciceToSVG(){
  precice-config-visualizer "${1:-precice-config.xml}" | dot -Tsvg > precice-config.svg
}
